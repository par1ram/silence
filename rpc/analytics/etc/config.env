# Analytics Service Configuration

# HTTP Server
HTTP_PORT=:8084
LOG_LEVEL=info
VERSION=1.0.0

# InfluxDB Configuration
INFLUXDB_URL=http://localhost:8086
INFLUXDB_TOKEN=your-influxdb-token
INFLUXDB_ORG=your-org
INFLUXDB_BUCKET=analytics

# Redis Configuration (for dashboards and alerts)
REDIS_ADDR=localhost:6379
REDIS_PASSWORD=
REDIS_DB=0

# External Services URLs (for metrics collection)
VPN_CORE_URL=http://localhost:8082
GATEWAY_URL=http://localhost:8080
DPI_BYPASS_URL=http://localhost:8083
AUTH_URL=http://localhost:8081

# Metrics Collection
METRICS_COLLECTION_INTERVAL=30s
METRICS_COLLECTION_ENABLED=true

# Alert Service
ALERT_EVALUATION_INTERVAL=60s
ALERT_EVALUATION_ENABLED=true 